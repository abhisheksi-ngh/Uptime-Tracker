{
    "manifest_version": 3,
    "name": "Uptime Tracker",
    "version": "1.0",
    "description": "Laptop uptime display and tracker extension.",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}
